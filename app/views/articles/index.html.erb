<% @articles.each do |article| %>
    <h1><%= link_to article.title, article %></h1>
    <p><%= article.body%></p>
    <p><%= article.visits_count%></p>
    <%= link_to "Eliminar", article, method: :delete, class: "btn btn-block btn-danger"%>
    <%= link_to "Editar", edit_article_path(article), class: "btn btn-block btn-success"%>
<%end%>